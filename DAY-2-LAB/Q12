class Parent {
    int x = 10;
}

class Child extends Parent {
    int x = 20;

    public void display() {
        System.out.println("Child x: " + x);
        System.out.println("Parent x: " + super.x); // 'super' accesses parent class variable
    }
}

public class Main {
    public static void main(String[] args) {
        Child obj = new Child();
        obj.display();
    }
}
